<h2>Available drones</h2>
{{#each drones}}
<div>
  <p>name:{{this.name}}</p>
  <p>Propellers:{{this.propellers}}</p> 
  <p>Speed:{{this.maxSpeed}}</p>
</div>
<div>
  <a href="/drones/{{this._id}}/edit"><button>Edit</button></a>
  <form method="POST" action="/drones/{{this._id}}/delete">
    <button>Delete</button>
  </form>
</div>
{{/each}}